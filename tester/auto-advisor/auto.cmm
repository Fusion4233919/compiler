int name[101][101] ;
int credit[101] ;
int score[101] ;
int tried[101] ;
int pre[101][101][101][101] ;

int curPreSet[101][101] ;
int curPre[101] ;
int cur[101] ;

int SumScore ;
int SumCreditTried ;
int SumCreditGet ;
int GratuateCredit ;

function int _main(void)
declare
	int tmp ;
	int cnt ;

	int tag ;
	int found ;
	int tagCur ;

	int i, j, k, l, ii, hhh ;


	int digitA, digitB, digitC ;

	/*		 id	id_string  */
do
	SumScore = 0 ;
	SumCreditTried = 0 ;
	SumCreditGet = 0 ;
	GratuateCredit = 0 ;
	
	tmp = 0 ;
	cnt = 1 ;
	_input("%c", &tmp) ;

	loop(1 == 1) do
		if (tmp == '\n' || tmp == '\r' ) do
			break ;
		done
		name[cnt][0] = 0 ;
		loop (tmp != '|' ) do
			name[cnt][0] = name[cnt][0] + 1 ;
			name[cnt][ name[cnt][0] ] = tmp ;
			_input("%c", &tmp) ;
		done

		_input("%d", &credit[cnt]) ;

		_input("%c", &tmp) ;
		_input("%c", &tmp) ;

		loop (tmp != '|' ) do
			curPreSet[0][0] = 0 ;

			loop ( tmp != ';' && tmp != '|') do
				curPre[0] = 0 ;
			
				loop ( tmp != ',' && tmp != ';' && tmp != '|') do
					curPre[0] = curPre[0] + 1 ;
					curPre[curPre[0]] = tmp ;
					_input("%c", &tmp) ;
				done
				curPreSet[0][0] = curPreSet[0][0] + 1 ;
				curPreSet[ curPreSet[0][0] ][0] = curPre[0] ;

				i = 1 ;
				loop (i <= curPre[0]) do
					curPreSet[ curPreSet[0][0] ][i] = curPre[i] ;
					i = i + 1 ;
				done
				if (tmp == ',') do
					_input("%c", &tmp) ;
				done
			done

			pre[cnt][0][0][0] = pre[cnt][0][0][0] + 1 ;
			pre[cnt][ pre[cnt][0][0][0] ][0][0] = curPreSet[0][0] ;

			i = 1 ;
			loop (i <= curPreSet[0][0] ) do
				pre[cnt][ pre[cnt][0][0][0] ][i][0] = curPreSet[i][0] ;
				j = 1 ;
				loop (j <= curPreSet[i][0] ) do
					pre[cnt][ pre[cnt][0][0][0] ][i][j] = curPreSet[i][j] ;
					j = j + 1 ;
				done
				i = i + 1 ;
			done

			if (tmp == ';') do
				_input("%c", &tmp) ;
			done
		done

		_input("%c", &tmp) ;
		tried[cnt] = 1 ;

		if (tmp == 'A') do
			score[cnt] = 4 ;
		done
		if (tmp == 'B') do
			score[cnt] = 3 ;
		done
		if (tmp == 'C') do
			score[cnt] = 2 ;
		done
		if (tmp == 'D') do
			score[cnt] = 1 ;
		done
		if (tmp == 'F') do
			score[cnt] = 0 ;
		done
		if (tmp != 'A' && tmp != 'B' && tmp != 'C' && tmp !='D' && tmp != 'E' && tmp != 'F' ) do
			tried[cnt] = 0 ;
			score[cnt] = 0 ;
		done

		if (tmp == 'A' || tmp == 'B' || tmp == 'C' || tmp =='D' || tmp == 'E' || tmp == 'F' ) do
			_input("%c", &tmp) ;
		done

		GratuateCredit = GratuateCredit + credit[cnt] ;
		if (tried[cnt] != 0) do
			SumCreditTried = SumCreditTried  + credit[cnt] ;
			SumScore = SumScore + score[cnt] * credit[cnt];
			if (score[cnt] > 0) do
				SumCreditGet = SumCreditGet + credit[cnt] ;
			done
		done

		_input("%c", &tmp) ;
		cnt = cnt + 1;

	done

	cnt = cnt - 1 ;

	if ( SumCreditTried == 0 ) do
		_output("GPA: 0.0\n") ;
	done

	if ( SumCreditTried != 0 ) do
		digitA = SumScore / SumCreditTried ;
		digitB = ((SumScore * 10) / SumCreditTried) % 10 ;
		digitC = ((SumScore * 100) / SumCreditTried) % 10 ;
		if (digitC >= 5) do
			digitB = digitB + 1 ;
		done

		_output("GPA: %d.%d\n", digitA, digitB) ;
	done
	_output("Hours Attempted: %d\n", SumCreditTried) ;
	_output("Hours Completed: %d\n", SumCreditGet) ;
	
	digitA = GratuateCredit - SumCreditGet ;
	_output("Credits Remaining: %d\n\n", digitA) ;
	_output("Possible Courses to Take Next\n") ;

	if (GratuateCredit == SumCreditGet ) do
		_output("  None - Congratulations!\n") ;
		return 0 ;
	done
	if (GratuateCredit != SumCreditGet ) do
		i = 1 ;
		loop ( i <= cnt ) do
			if (score[i] == 0) do
				tag = 0 ;
				if (pre[i][0][0][0] == 0) do
					tag = 1 ;
				done

				j = 1 ;
				loop ( j <= pre[i][0][0][0] ) do
					tag = 1 ;

					k = 1 ;
					loop ( k <= pre[i][j][0][0] ) do
						cur[0] = pre[i][j][k][0] ;

						ii = 1 ;
						loop (ii <= pre[i][j][k][0]) do
							cur[ii] = pre[i][j][k][ii] ;
							ii = ii + 1 ;
						done

						found = 0 ;
						
						l = 1 ;
						loop ( l <= cnt ) do
							tagCur = 1 ;

							if (name[l][0] != cur[0] ) do
								tagCur = 0 ;
							done
							
							if (name[l][0] == cur[0] ) do
								ii = 1 ;
								loop (ii <= cur[0]) do
									if ( name[l][ii] != cur[ii] ) do
										tagCur = 0 ;
										break ;
									done
									ii = ii + 1 ;
								done
							done

							if (tagCur != 0) do
								found = 1 ;
								if (score[l] == 0) do
									tag = 0 ;
								done
								break ;
							done

							l = l + 1 ;
						done
						if (found == 0) do
							tag = 0 ;
						done

						k = k + 1 ;
					done
					if ( tag == 1 ) do
						break ;
					done

					j = j + 1 ;
				done
				if ( tag == 1 ) do
					_output("  ") ;

					hhh = 1 ;
					loop (hhh <= name[i][0]) do
						_output("%c", name[i][hhh]) ;
						hhh = hhh + 1 ;
					done
					_output("\n") ;
				done

			done
			i = i + 1 ;
		done
	
	done
	return 0 ;
done
